{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "cod": {
      "type": "string"
    },
    "count": {
      "type": "integer"
    },
    "list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "coord": {
            "type": "object",
            "properties": {
              "lat": {
                "type": [
                  "number",
                  "integer"
                ]
              },
              "lon": {
                "type": "number"
              }
            },
            "additionalProperties": true,
            "required": [
              "lat",
              "lon"
            ]
          },
          "main": {
            "type": "object",
            "properties": {
              "temp": {
                "type": [
                  "number",
                  "integer"
                ]
              },
              "feels_like": {
                "type": "number"
              },
              "temp_min": {
                "type": [
                  "number",
                  "integer"
                ]
              },
              "temp_max": {
                "type": [
                  "number",
                  "integer"
                ]
              },
              "pressure": {
                "type": "integer"
              },
              "humidity": {
                "type": "integer"
              },
              "sea_level": {
                "type": "integer"
              },
              "grnd_level": {
                "type": "integer"
              }
            },
            "additionalProperties": true,
            "required": [
              "temp",
              "feels_like",
              "temp_min",
              "temp_max",
              "pressure",
              "humidity"
            ]
          },
          "dt": {
            "type": "integer"
          },
          "wind": {
            "type": "object",
            "properties": {
              "speed": {
                "type": [
                  "number",
                  "integer"
                ]
              },
              "deg": {
                "type": "integer"
              }
            },
            "additionalProperties": true,
            "required": [
              "speed",
              "deg"
            ]
          },
          "sys": {
            "type": "object",
            "properties": {
              "country": {
                "type": "string"
              }
            },
            "additionalProperties": true,
            "required": [
              "country"
            ]
          },
          "rain": {
            "type": "null"
          },
          "snow": {
            "type": "null"
          },
          "clouds": {
            "type": "object",
            "properties": {
              "all": {
                "type": "integer"
              }
            },
            "additionalProperties": true,
            "required": [
              "all"
            ]
          },
          "weather": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "main": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "icon": {
                  "type": "string"
                }
              },
              "additionalProperties": true,
              "required": [
                "id",
                "main",
                "description",
                "icon"
              ]
            },
            "additionalItems": true
          }
        },
        "additionalProperties": true,
        "required": [
          "id",
          "name",
          "coord",
          "main",
          "dt",
          "wind",
          "sys",
          "rain",
          "snow",
          "clouds",
          "weather"
        ]
      },
      "additionalItems": true
    }
  },
  "additionalProperties": true,
  "required": [
    "message",
    "cod",
    "count",
    "list"
  ]
}